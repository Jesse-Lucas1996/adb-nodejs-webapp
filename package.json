{
  "name": "android-tv-ts",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsc && yarn copy-assets && yarn build-sass && yarn post-build",
    "post-build": "node build/post-build.js",
    "start": "node build/index.js",
    "watch": "nodemon",
    "test": "mocha -t 20000 --reporter spec --exit 'build/**/*.spec.js'",
    "testonly": "mocha -t 20000 --exit 'build/**/*.spec.js'",
    "build-start": "yarn build && yarn start",
    "copy-assets": "rsync -rv --exclude=*.scss src/website/views build/website/ && rsync -rv src/website/images build/website/ && rsync -rv src/apks build/",
    "build-sass": "node-sass ./src/website/views -o ./build/website/views"
  },
